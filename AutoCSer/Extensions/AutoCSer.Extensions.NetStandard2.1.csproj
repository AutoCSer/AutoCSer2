<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>AutoCSer.Extensions</AssemblyName>
    <RootNamespace>AutoCSer.Extensions</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>C:\AutoCSer2\AutoCSer\Extensions\AutoCSer.Extensions.xml</DocumentationFile>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DocumentationFile>C:\AutoCSer2\AutoCSer\Extensions\AutoCSer.Extensions.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="**" />
    <EmbeddedResource Remove="**" />
    <None Remove="**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="ArrayHeap.cs" />
    <Compile Include="Date.cs" />
    <Compile Include="Deploy\SwitchProcess.cs" />
    <Compile Include="Deploy\SwitchWait.cs" />
    <Compile Include="Extensions\SemaphoreSlimLock.cs" />
    <Compile Include="FifoPriorityQueue.cs" />
    <Compile Include="FragmentDictionary256.cs" />
    <Compile Include="FragmentHashSet256.cs" />
    <Compile Include="Net\TimestampVerify.cs" />
    <Compile Include="ObjectRoot\ObjectReference.cs" />
    <Compile Include="ObjectRoot\ObjectType.cs" />
    <Compile Include="ObjectRoot\ReflectionArrayScanner.cs" />
    <Compile Include="ObjectRoot\ReflectionArrayType.cs" />
    <Compile Include="ObjectRoot\ReflectionObjectScanner.cs" />
    <Compile Include="ObjectRoot\ReflectionObjectType.cs" />
    <Compile Include="ObjectRoot\ReflectionScanner.cs" />
    <Compile Include="ObjectRoot\ReflectionType.cs" />
    <Compile Include="ObjectRoot\ReflectionTypeScanner.cs" />
    <Compile Include="ObjectRoot\TypeScanner.cs" />
    <Compile Include="SearchTree\Dictionary.cs" />
    <Compile Include="SearchTree\FindArray.cs" />
    <Compile Include="SearchTree\Node.cs" />
    <Compile Include="SearchTree\PageArray.cs" />
    <Compile Include="SearchTree\Set.cs" />
    <Compile Include="Threading\DistributedMillisecondIdentityGenerator.cs" />
    <Compile Include="Threading\LocalMillisecondIdentityGenerator.cs" />
    <Compile Include="Threading\MillisecondIdentityGenerator.cs" />
    <Compile Include="Threading\ReentrantSemaphoreSlimLockManager.cs" />
    <Compile Include="Threading\ReentrantSemaphoreSlimLockCount.cs" />
    <Compile Include="Threading\TaskRunTimer.cs" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="AutoCSer">
      <HintPath>..\bin\$(ConfigurationName)\netstandard2.1\AutoCSer.dll</HintPath>
    </Reference>
  </ItemGroup>
  
</Project>
