<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <AssemblyName>AutoCSer.TestCase.ReverseLogCollectionClient</AssemblyName>
    <RootNamespace>AutoCSer.TestCase.ReverseLogCollectionClient</RootNamespace>
    <LangVersion>7.1</LangVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <Optimize>true</Optimize>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="**" />
    <EmbeddedResource Remove="**" />
    <None Remove="**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="CommandClientConfig.cs" />
    <Compile Include="CommandClientSocketEvent.cs" />
    <Compile Include="Program.cs" />
    <Compile Include="ReverseLogCollectionClient.cs" />
    <Compile Include="ReverseLogCollectionClientServiceRegistrar.cs" />
    <Compile Include="ServiceRegistryCommandClientConfig.cs" />
    <Compile Include="ServiceRegistryCommandClientSocketEvent.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Application\ReverseLogCollection\AutoCSer.CommandService.ReverseLogCollection.NetStandard2.1.csproj" />
    <ProjectReference Include="..\..\..\Application\ServiceRegistry\AutoCSer.CommandService.ServiceRegistry.NetStandard2.1.csproj" />
    <ProjectReference Include="..\..\..\Application\TimestampVerify\AutoCSer.CommandService.TimestampVerify.NetStandard2.1.csproj" />
    <ProjectReference Include="..\..\Common\AutoCSer.TestCase.Common.NetStandard.csproj" />
    <ProjectReference Include="..\Common\AutoCSer.TestCase.ReverseLogCollectionCommon.NetStandard.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="AutoCSer">
      <HintPath>..\..\..\AutoCSer\bin\$(ConfigurationName)\netstandard2.1\AutoCSer.dll</HintPath>
    </Reference>
  </ItemGroup>
  
</Project>
