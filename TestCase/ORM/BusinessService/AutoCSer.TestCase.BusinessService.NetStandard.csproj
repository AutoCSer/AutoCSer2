<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>AutoCSer.TestCase.BusinessService</AssemblyName>
    <RootNamespace>AutoCSer.TestCase.BusinessService</RootNamespace>
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="**" />
    <EmbeddedResource Remove="**" />
    <None Remove="**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="AutoIdentityModel.cs" />
    <Compile Include="BusinessService\AutoIdentityModelService.cs" />
    <Compile Include="BusinessService\CustomColumnFieldModelService.cs" />
    <Compile Include="BusinessService\CustomColumnPropertyModelService.cs" />
    <Compile Include="BusinessService\FieldModelService.cs" />
    <Compile Include="BusinessService\IAutoIdentityService.cs" />
    <Compile Include="BusinessService\IBaseService.cs" />
    <Compile Include="BusinessService\IPrimaryKeyService.cs" />
    <Compile Include="BusinessService\PropertyModelService.cs" />
    <Compile Include="CustomColumnFieldModel.cs" />
    <Compile Include="CustomColumnPropertyModel.cs" />
    <Compile Include="FieldModel.cs" />
    <Compile Include="Persistence.cs" />
    <Compile Include="Program.cs" />
    <Compile Include="PropertyModel.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Application\DistributedLock\AutoCSer.CommandService.DistributedLock.NetStandard2.1.csproj" />
    <ProjectReference Include="..\..\..\Application\TimestampVerify\AutoCSer.CommandService.TimestampVerify.NetStandard2.1.csproj" />
    <ProjectReference Include="..\CommonModel\AutoCSer.TestCase.CommonModel.NetStandard.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="AutoCSer">
      <HintPath>..\..\..\AutoCSer\bin\$(ConfigurationName)\netstandard2.1\AutoCSer.dll</HintPath>
    </Reference>
  </ItemGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="if exist &quot;$(ProjectDir)..\..\..\AutoCSer\CodeGenerator\bin\$(ConfigurationName)\netcoreapp3.1\AutoCSer.CodeGenerator.exe&quot; $(ProjectDir)..\..\..\AutoCSer\CodeGenerator\bin\$(ConfigurationName)\netcoreapp3.1\AutoCSer.CodeGenerator.exe &quot;$(ProjectName)&quot; &quot;$(ProjectDir) &quot; &quot;$(TargetPath) &quot; &quot;$(TargetName)&quot;" />
  </Target>

</Project>
